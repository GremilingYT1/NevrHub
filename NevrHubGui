-- Scripts:

local function YLVHHH_fake_script() -- Continue_Free.ButtonFunctionality 
	local script = Instance.new('LocalScript', Continue_Free)

	-- Variables
	
	local Button = script.Parent
	local StarterScreenFrame = script.Parent.Parent.Parent.StarterScreenInfo
	local HubFrame = script.Parent.Parent.Parent.MainFrame
	
	-- Code
	
	HubFrame.Visible = false
	
	Button.MouseButton1Click:Connect(function()
		StarterScreenFrame.Visible = false
		HubFrame.Visible = true
	end)
end
coroutine.wrap(YLVHHH_fake_script)()
local function FRYZH_fake_script() -- Continue_Ads.ButtonFunctionality 
	local script = Instance.new('LocalScript', Continue_Ads)

	-- Variables
	
	local Button = script.Parent
	local StarterScreenFrame = script.Parent.Parent.Parent.StarterScreenInfo
	local HubFrame = script.Parent.Parent.Parent.MainFrame
	local somethingTocopy = "www.google.com"
	
	-- Code
	
	HubFrame.Visible = false
	
	Button.MouseButton1Click:Connect(function()
		setclipboard(somethingTocopy)
		StarterScreenFrame.Visible = false
		HubFrame.Visible = true
	end)
end
coroutine.wrap(FRYZH_fake_script)()
local function NNZU_fake_script() -- CloseButton.CloseFunction 
	local script = Instance.new('LocalScript', CloseButton)

	-- Variables
	
	local Button = script.Parent
	local Frame = script.Parent.Parent.Parent.ContentHolder
	local Frametop = script.Parent.Parent.Parent.TopBar
	
	-- Code
	
	Button.MouseButton1Click:Connect(function()
		Frame.Visible = false
		Frametop.Visible = false
	end)
end
coroutine.wrap(NNZU_fake_script)()
local function PJTX_fake_script() -- Button1.Script 
	local script = Instance.new('LocalScript', Button1)

	-- Variables
	
	local Button = script.Parent
	
	-- Code
	
	Button.MouseButton1Click:Connect(function()
		_G.HeadSize = 10 _G.Disabled = true game:GetService('RunService').RenderStepped:connect(function() if _G.Disabled then for i,v in next, game:GetService('Players'):GetPlayers() do if v.Name ~= game:GetService('Players').LocalPlayer.Name then pcall(function() v.Character.Head.Size = Vector3.new(_G.HeadSize,_G.HeadSize,_G.HeadSize) v.Character.Head.Transparency = 1 v.Character.Head.BrickColor = BrickColor.new("Red") v.Character.Head.Material = "Neon" v.Character.Head.CanCollide = false v.Character.Head.Massless = true end) end end end end)
	end)
end
coroutine.wrap(PJTX_fake_script)()
local function JQYJ_fake_script() -- MainFrame.DragFrame 
	local script = Instance.new('LocalScript', MainFrame)

	-- LocalScript in the Frame
	local UIS = game:GetService("UserInputService")
	local TweenService = game:GetService("TweenService")
	local frame = script.Parent
	
	local dragging = false
	local dragInput, mouseStart, frameStart
	local dragSpeed = 0.15  -- Duration of the easing in seconds
	local easingStyle = Enum.EasingStyle.Quad
	local easingDirection = Enum.EasingDirection.Out
	
	local function updatePosition(input)
		local delta = input.Position - mouseStart
		local newPosition = UDim2.new(
			frameStart.X.Scale,
			frameStart.X.Offset + delta.X,
			frameStart.Y.Scale,
			frameStart.Y.Offset + delta.Y
		)
		local tweenInfo = TweenInfo.new(dragSpeed, easingStyle, easingDirection)
		local tween = TweenService:Create(frame, tweenInfo, {Position = newPosition})
		tween:Play()
	end
	
	frame.InputBegan:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
			dragging = true
			mouseStart = input.Position
			frameStart = frame.Position
	
			input.Changed:Connect(function()
				if input.UserInputState == Enum.UserInputState.End then
					dragging = false
				end
			end)
		end
	end)
	
	frame.InputChanged:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch then
			dragInput = input
		end
	end)
	
	UIS.InputChanged:Connect(function(input)
		if input == dragInput and dragging then
			updatePosition(input)
		end
	end)
	
end
coroutine.wrap(JQYJ_fake_script)()
